{
  "name": "zoomies",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:backend\" \"pnpm:dev:frontend\" --names \"backend,frontend\" --prefix-colors \"cyan,magenta\"",
    "dev:frontend": "pnpm -C apps/frontend dev",
    "dev:backend": "pnpm -C apps/backend dev",
    "start": "concurrently \"pnpm:start:backend\" \"pnpm:start:frontend\" --names \"backend,frontend\" --prefix-colors \"cyan,magenta\"",
    "start:frontend": "pnpm -C apps/frontend build && pnpm -C apps/frontend preview",
    "start:backend": "pnpm -C apps/backend start",
    "build": "pnpm -C apps/frontend build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "node-fetch": "^3.3.2"
  }
}
